<view class="zai-box"><image class="zai-logo" src="../../static/face.jpg" mode="aspectFit"></image><view class="zai-title"></view><view class="zai-form"><input class="zai-input" placeholder-class placeholder="请输入账号" data-event-opts="{{[['input',[['__set_model',['','account','$event',[]]]]]]}}" value="{{account}}" bindinput="__e"/><input class="zai-input" placeholder-class password placeholder="请输入密码" data-event-opts="{{[['input',[['__set_model',['','password','$event',[]]]]]]}}" value="{{password}}" bindinput="__e"/><view class="zai-verify"><move-verify bind:result="__e" vue-id="1" data-event-opts="{{[['^result',[['verifyResult']]]]}}" bind:__l="__l"></move-verify></view><button data-event-opts="{{[['tap',[['bindLogin',['$event']]]]]}}" class="zai-btn" bindtap="__e">立即登录</button><view class="zai-label"><text data-event-opts="{{[['tap',[['forgetPassword',['$event']]]]]}}" bindtap="__e">忘记密码</text><text data-event-opts="{{[['tap',[['registerUser',['$event']]]]]}}" class="register" bindtap="__e">点此注册</text></view><view class="version">{{"当前版本:"+version}}</view></view><w-loading class="vue-ref" vue-id="2" text="加载中.." mask="true" click="false" data-ref="loading" bind:__l="__l"></w-loading><yu-toast class="vue-ref" vue-id="3" message="{{message}}" verticalAlign="center" data-ref="toast" bind:__l="__l"></yu-toast></view>