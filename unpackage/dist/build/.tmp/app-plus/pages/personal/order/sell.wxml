<view class="main"><view class="uni-list"><view class="uni-list-cell"><view class="uni-list-cell-left">出售资产</view><view class="uni-list-cell-db"><picker value="{{index}}" range="{{array}}" range-key="name" data-event-opts="{{[['change',[['bindPickerChange',['$event']]]]]}}" bindchange="__e"><view class="uni-input">{{array[index].name}}</view></picker></view></view></view><view class="main-list"><input-box bind:input="__e" vue-id="1" placeholder="出售资产" leftText="出售资产:" value="{{body.price}}" data-event-opts="{{[['^input',[['__set_model',['$0','price','$event',[]],['body']]]]]}}" bind:__l="__l"></input-box><input-box bind:input="__e" vue-id="2" placeholder="二级密码" leftText="二级密码:" value="{{password}}" data-event-opts="{{[['^input',[['__set_model',['','password','$event',[]]]]]]}}" bind:__l="__l"></input-box></view><view class="view-btn" style="padding-left:20rpx;padding-right:20rpx;margin-top:20rpx;"><button type="primary" data-event-opts="{{[['tap',[['toSell',['$event']]]]]}}" bindtap="__e">确认</button></view><yu-toast class="vue-ref" vue-id="3" message="{{message}}" verticalAlign="center" data-ref="toast" bind:__l="__l"></yu-toast></view>